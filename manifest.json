{
    "name": "Lync",
    "version": "1.0.0",
    "description": "Sync links across browers",
    "manifest_version": 2,
    "author": "Rahul V",
    "browser_action":{
        "default_popup": "index.html", 
        "default_title": "Lync"
    },
    "background":{
        "scripts":["background.js"],
        "persistent" : true
    },
    "permissions":[
        "tabs",
        "storage",
        "identity",
        "identity.email"
    ]
}